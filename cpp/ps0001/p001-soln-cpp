#include <iostream> 
using namespace std;
int main() 
{ int number_one;
  cin>>number_one;
  int number_two;
  cin>>number_two;
  int number_three;
  cin>>number_three;
  int Summation=number_one+number_two+number_three;
  cout<<"The sumation of three numbers ="<<Summation;
  
  return 0; 
}

